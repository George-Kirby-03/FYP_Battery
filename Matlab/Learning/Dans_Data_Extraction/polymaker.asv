function poly = polymaker(polycount, range, lower_bound)
%POLYMAKER Quick Function To Produce the Polynomial Coefficent & indexes
%NOTE: R0, R1, C, Q in that order AFTER

poly.xl = -ones(polycount)*range;
poly.xe = zaeros(polycount);
poly.xu = ones(polycount)*range;
poly.xl(1) = lower_bound - 0.2;
poly.xe(1) = poly.xl(1);
poly.xu(1) = poly.xe(1); 
poly.R0 = polycount+1;
poly.R1 = polycount+2;
poly.C  = polycount+3;
poly.Q = polycount+4;
end